model Job {
    id                Int             @id @default(autoincrement())
    enabled           Boolean         @db.Boolean @default(true)
    title             String          @db.VarChar(250) @unique
    createdAt         DateTime        @default(now()) @map("created_at")
    updatedAt         DateTime        @default(now()) @map("updated_at")

    @@map("jobs")
}
