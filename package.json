{
  "name": "cvgenerator",
  "version": "1.0.0",
  "description": "# Project commands",
  "type": "module",
  "main": "server.ts",
  "scripts": {
    "start": "node --trace-warnings --loader ts-node/esm --inspect src/server.ts",
    "dev": "nodemon --exec node --loader ts-node/esm src/server.ts",
    "db:migrate": "npx prisma migrate dev --name user-entity --create-only && npx prisma generate",
    "db:push": "npx prisma db push"
  },
  "compilerOptions": {
    "target": "es5",
    "module": "ESNext",
    "lib": [
      "es6",
      "dom"
    ]
  },
  "keywords": [
    "nodejs",
    "express",
    "prisma",
    "rest",
    "api",
    "postgresql"
  ],
  "author": "Goran SubiÄ‡ <goran.subic@actdigital.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "prisma": "^5.20.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.19.2",
    "express-handlebars": "^7.1.2",
    "nodemon": "^3.1.4",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "pgsql": "^1.0.0",
    "ts-node": "^10.9.2"
  }
}
